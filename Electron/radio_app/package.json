{
  "name": "radio_app",
  "productName": "Radio",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": {
    "name": "Alex Chen",
    "email": "62322279+dreamct25@users.noreply.github.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon":"./src/icon/radio-waves-icon"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "radio_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "loggerPort": 9001,
            "mainConfig": "./webpack.main.config.js",
            "devContentSecurityPolicy": "connect-src * http://localhost:9870 'unsafe-eval'",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.63",
    "@types/sqlite3": "^3.1.8",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.0",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^6.5.1",
    "electron": "16.0.7",
    "electron-rebuild": "^3.2.7",
    "eslint": "^8.7.0",
    "eslint-plugin-import": "^2.25.4",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "node-loader": "^2.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.0",
    "@types/styled-components": "^5.1.21",
    "@types/video.js": "^7.3.29",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "electron-prebuilt-compile": "^8.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.1",
    "hinet-hichannel-taiwan-radio": "^3.1.0",
    "i18next": "^21.6.7",
    "internet-available": "^1.0.0",
    "morgan": "^1.10.0",
    "react": "^17.0.2",
    "react-circle-slider": "^1.6.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.3",
    "react-router-dom": "^5.3.0",
    "sqlite3": "^5.0.0",
    "styled-components": "^5.3.3",
    "video.js": "^7.17.0",
    "videojs-contrib-hls": "^5.15.0"
  }
}
